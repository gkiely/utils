{
  "version": "0.1.21",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bench": "vite-node test/benchmark",
    "start": "tsdx watch",
    "build": "tsdx build",
    "build-tsc": "tsc --noEmit false",
    "test-all": "tsdx test",
    "test": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "pub": "npm version patch --force && npm run prepare && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gkiely/utils.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gkiely/utils",
  "author": "Grant Kiely",
  "module": "dist/utils.esm.js",
  "size-limit": [
    {
      "path": "dist/utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "benchmark": "^2.1.4",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vite-node": "^0.23.4"
  }
}
